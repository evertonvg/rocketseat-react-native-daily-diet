{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@types/*": ["./src/@types/*"],
      "@assets/*":["./src/assets/*"],
      "@components/*":["./src/components/*"],
      "@screens/*":["./src/screens/*"],
      "@routes/*":["./src/routes/*"],
      "@storage/*":["./src/storage/*"],
      "@utils/*":["./src/utils/*"],
      "@hooks/*":["./src/hooks/*"],
      "@services/*":["./src/services/*"],
      "@contexts/*":["./src/contexts/*"],
      "@theme/*":["./src/theme/*"]
    },
  }
}
